{
  "name": "my-personal-website",
  "homepage": "https://micahkepe.com/",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@heroicons/react": "^2.1.5",
    "@tailwindcss/forms": "^0.5.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "classnames": "^2.3.2",
    "framer-motion": "^11.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.27.0",
    "react-scroll": "^1.8.9",
    "vite": "^5.4.10",
    "web-vitals": "^4.2.3"
  },
  "scripts": {
    "start": "npm run build:blog-dev && vite",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "preview-prod": "npm run predeploy && vite preview",
    "build:blog-dev": "rm -rf public/blog && cd blog && zola build --base-url http://localhost:3000/blog && cd .. && cp -r blog/public/ public/blog/",
    "build:blog": "rm -rf public/blog && cd blog && zola build && cd .. && cp -r blog/public/ public/blog/",
    "format": "prettier --write .",
    "test": "vite test",
    "clean": "rm -rf build/*",
    "predeploy": "npm run clean && npm run build:blog && npm run build",
    "deploy": "gh-pages -d build -m 'deploying to gh-pages'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/preset-react": "^7.25.9",
    "@svgr/cli": "^8.0.1",
    "@tailwindcss/typography": "^0.5.15",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "gh-pages": "^6.1.1",
    "prettier": "^3.3.3",
    "sass": "^1.80.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
